<RelativeLayout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="bottom"
    tools:context="kr.hansung.mypi.ResultActivity" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bg03"
        android:orientation="vertical" >

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="20dp"
            android:fillViewport="true" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:background="#00ffffff"
                android:orientation="vertical" >

                <LinearLayout
                    android:id="@+id/listLayout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#55ffffff"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center|center_vertical"
                        android:orientation="vertical" >

                        <ImageView
                            android:id="@+id/imageView2"
                            android:layout_width="150dp"
                            android:layout_height="50dp"
                            android:src="@drawable/logo_300x100" />

                        <TextView
                            android:id="@+id/textView2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="2014-06-14   tera16님의 검색 결과"
                            android:textColor="#ffffff" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="10dp"
                        android:gravity="center|center_vertical"
                        android:orientation="vertical" >

                        <LinearLayout
                            android:layout_width="250dp"
                            android:layout_height="2dp"
                            android:background="#ffffff"
                            android:orientation="vertical" >
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center|center_vertical"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/gradeText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="" />

                        <TextView
                            android:id="@+id/gradeExpText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="검색된 결과를 솔루션을 통해 지워보세요"
                            android:textColor="#FAF4C0"
                            android:textSize="10sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="10dp"
                        android:gravity="center|center_vertical"
                        android:orientation="vertical" >

                        <LinearLayout
                            android:layout_width="250dp"
                            android:layout_height="2dp"
                            android:background="#ffffff"
                            android:orientation="vertical" >
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center|center_vertical" >

                            <TextView
                                android:id="@+id/TextView01"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="검색엔진그래프" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center|center_vertical" >

                            <TextView
                                android:id="@+id/textView5"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="노출관리그래프" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="10dp"
                        android:gravity="center|center_vertical"
                        android:orientation="vertical" >

                        <LinearLayout
                            android:layout_width="250dp"
                            android:layout_height="2dp"
                            android:background="#ffffff"
                            android:orientation="vertical" >
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center|center_vertical"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/textView4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="MY-Pi 솔루션을 통해 찾아낸  \n회원님과 관련된 이미지 검색 결과입니다."
                            android:textColor="#ffffff" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center|center_vertical"
                        android:orientation="vertical" >

                        <GridLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:columnCount="5"
                            android:orientation="vertical"
                            android:rowCount="2" >

                            <ImageView
                                android:id="@+id/imgResult0"
                                android:layout_width="50dp"
                                android:layout_height="40dp"
                                android:layout_column="0"
                                android:layout_gravity="left"
                                android:layout_row="0"
                                android:src="@drawable/image" />

                            <ImageView
                                android:id="@+id/imgResult1"
                                android:layout_width="50dp"
                                android:layout_height="40dp"
                                android:layout_column="1"
                                android:layout_gravity="left"
                                android:layout_row="0"
                                android:src="@drawable/image" />

                            <ImageView
                                android:id="@+id/imgResult2"
                                android:layout_width="50dp"
                                android:layout_height="40dp"
                                android:layout_column="2"
                                android:layout_gravity="left"
                                android:layout_row="0"
                                android:src="@drawable/image" />

                            <ImageView
                                android:id="@+id/imgResult3"
                                android:layout_width="50dp"
                                android:layout_height="40dp"
                                android:layout_column="3"
                                android:layout_gravity="left"
                                android:layout_row="0"
                                android:src="@drawable/image" />

                            <ImageView
                                android:id="@+id/imgResult4"
                                android:layout_width="50dp"
                                android:layout_height="40dp"
                                android:layout_column="4"
                                android:layout_gravity="left"
                                android:layout_row="0"
                                android:src="@drawable/image" />

                            <ImageView
                                android:id="@+id/imgResult5"
                                android:layout_width="50dp"
                                android:layout_height="40dp"
                                android:layout_column="0"
                                android:layout_gravity="left"
                                android:layout_row="1"
                                android:src="@drawable/image" />

                            <ImageView
                                android:id="@+id/imgResult6"
                                android:layout_width="50dp"
                                android:layout_height="40dp"
                                android:layout_column="1"
                                android:layout_gravity="left"
                                android:layout_row="1"
                                android:src="@drawable/image" />

                            <ImageView
                                android:id="@+id/imgResult7"
                                android:layout_width="50dp"
                                android:layout_height="40dp"
                                android:layout_column="2"
                                android:layout_gravity="left"
                                android:layout_row="1"
                                android:src="@drawable/image" />

                            <ImageView
                                android:id="@+id/imgResult8"
                                android:layout_width="50dp"
                                android:layout_height="40dp"
                                android:layout_column="3"
                                android:layout_gravity="left"
                                android:layout_row="1"
                                android:src="@drawable/image" />

                            <ImageView
                                android:id="@+id/imgResult9"
                                android:layout_width="50dp"
                                android:layout_height="40dp"
                                android:layout_column="4"
                                android:layout_gravity="left"
                                android:layout_row="1"
                                android:src="@drawable/image" />
                        </GridLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="10dp"
                        android:gravity="center|center_vertical"
                        android:orientation="vertical" >

                        <LinearLayout
                            android:layout_width="250dp"
                            android:layout_height="2dp"
                            android:background="#ffffff"
                            android:orientation="vertical" >
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center|center_vertical"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/TextView02"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="MY-Pi 솔루션을 통해 찾아낸  \n주요 신상정보 노출사이트 목록입니다."
                            android:textColor="#ffffff" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="10dp"
                        android:gravity="center|center_vertical"
                        android:orientation="vertical" >

                        <LinearLayout
                            android:layout_width="250dp"
                            android:layout_height="2dp"
                            android:background="#ffffff"
                            android:orientation="vertical" >
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="10dp"
                        android:gravity="center|center_vertical"
                        android:orientation="vertical" >

                        <LinearLayout
                            android:layout_width="250dp"
                            android:layout_height="2dp"
                            android:background="#ffffff"
                            android:orientation="vertical" >
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center|center_vertical"
                        android:orientation="vertical" >

                        <Button
                            android:id="@+id/resultBtn"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Dynamic Search 결과"
                            android:textColor="#ffffff" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="10dp"
                        android:gravity="center|center_vertical"
                        android:orientation="vertical" >

                        <LinearLayout
                            android:layout_width="250dp"
                            android:layout_height="2dp"
                            android:background="#ffffff"
                            android:orientation="vertical" >
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>

</RelativeLayout>